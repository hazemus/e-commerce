{% extends "base.html.twig" %}

{% block body %}
    <div class="container">
        <table class="table">
            <tr>
                <th>Id</th>
                <th>Quantity</th>
                <th>Adding Date</th>
            </tr>
            {% for productAdded in productsAdded %}
                <tr>
                    <td>{{ productAdded.id }}</td>
                    <td>{{ productAdded.qte }}</td>
                    <td>{{ productAdded.createdAt|date }}</td>
                </tr>
                
            {% endfor %}
        </table>
    </div>
{% endblock %}